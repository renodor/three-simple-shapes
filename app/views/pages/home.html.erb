  <div class="h-line h-line-left"></div>
  <div class="triangles door2">
    <%= inline_svg_tag('triangle.svg', class: 'triangle') %>
    <%= inline_svg_tag('triangle2.svg', class: 'triangle') %>
  </div>
  <div class="v-line v-line-middle"></div>
  <div class="squares door3">
    <%= inline_svg_tag('square.svg', class: 'square') %>
    <%= inline_svg_tag('square2.svg', class: 'square') %>
  </div>
  <div class="v-line v-line-right"></div>
  <div class="h-line h-line-right"></div>

  <div class="v-line v-line-bottom"></div>

  <div class="key"></div>

  <div class="bells">
    <div class="bell">
      1
      <div data-id="1" class="bell-placeholder"></div>
    </div>
    <div class="bell">
            2
      <div data-id="2" class="bell-placeholder"></div>
    </div>
    <div class="bell">
            3
      <div data-id="3" class="bell-placeholder"></div>
